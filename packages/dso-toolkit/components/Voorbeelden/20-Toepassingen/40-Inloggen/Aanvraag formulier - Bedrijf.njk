<script src="https://www.google.com/recaptcha/api.js" async defer></script>
<main>
  {% render '@header' %}
  <h1>Aanvragen Omgevingsloket-ID</h1>
  <div class="row">
    <div class="col-md-8 col-sm-8 col-xs-12">
      <form class="dso-single-page">

        <fieldset>
          <legend>
            <h2>Formulier voor bedrijven</h2>
          </legend>

          <div class="form-group dso-input dso-input-text dso-required">
            <div class="dso-label-container">
              <label for="single-page-form-invoer-veld-1" class="control-label">
                Uw naam
              </label>
            </div>

            <div class="dso-field-container">
              <input type="text" id="single-page-form-invoer-veld-1" required class="form-control">
            </div>
          </div>

          <div class="form-group dso-input dso-input-text dso-required">
            <div class="dso-label-container">
              <label for="single-page-form-invoer-veld-2" class="control-label">
                Uw e-mailadres
              </label>
            </div>

            <div class="dso-field-container">
              <input type="text" id="single-page-form-invoer-veld-2" required class="form-control">
            </div>
          </div>

          <div class="form-group dso-input dso-input-text dso-required">
            <div class="dso-label-container">
              <label for="single-page-form-invoer-veld-2" class="control-label">
                Organisatie
              </label>
            </div>

            <div class="dso-field-container">
              <input type="text" id="single-page-form-invoer-veld-2" required class="form-control">
            </div>
          </div>

          <div class="form-group dso-input dso-input-text">
            <div class="dso-label-container">
              <label for="single-page-form-invoer-veld-2" class="control-label">
                Telefoon
              </label>
            </div>

            <div class="dso-field-container">
              <input type="text" id="single-page-form-invoer-veld-2" class="form-control">
            </div>
          </div>

          <div class="form-group dso-input dso-input-text dso-required">
            <div class="dso-label-container">
              <label for="single-page-form-invoer-veld-2" class="control-label">
                Adres
              </label>
            </div>

            <div class="dso-field-container">
              <input type="text" id="single-page-form-invoer-veld-2" required class="form-control">
            </div>
          </div>

          <div class="form-group dso-input dso-input-text ">
            <div class="dso-label-container">
              <label for="single-page-form-invoer-veld-2" class="control-label">
                Postcode
              </label>
            </div>

            <div class="dso-field-container">
              <input type="text" id="single-page-form-invoer-veld-2" class="form-control">
            </div>
          </div>

          <div class="form-group dso-input dso-input-text dso-required ">
            <div class="dso-label-container">
              <label for="single-page-form-invoer-veld-2" class="control-label">
                Plaats
              </label>
            </div>

            <div class="dso-field-container">
              <input type="text" id="single-page-form-invoer-veld-2" required class="form-control">
            </div>
          </div>

          <div class="form-group dso-input dso-input-text dso-required ">
            <div class="dso-label-container">
              <label for="single-page-form-invoer-veld-2" class="control-label">
                Land
              </label>
            </div>

            <div class="dso-field-container">
              <input type="text" id="single-page-form-invoer-veld-2" required class="form-control">
            </div>
          </div>

           <div class="form-group dso-input dso-input-text dso-required">
            <div class="dso-label-container">
              <label for="single-page-form-invoer-veld-2" class="control-label">
                Inschrijfnummer van uw bedrijf in buitenlands bedrijvenregister
              </label>
            </div>

            <div class="dso-field-container">
              <input type="text" id="single-page-form-invoer-veld-2" required class="form-control">
            </div>
          </div>

           <div class="form-group dso-input dso-input-text dso-required">
            <div class="dso-label-container">
              <label for="single-page-form-invoer-veld-2" class="control-label">
                Gegevens van bedrijvenregister(adres)
              </label>
            </div>

             <div class="dso-field-container">
              <input type="text" id="single-page-form-invoer-veld-2" required class="form-control">
            </div>
          </div>

           <div class="form-group dso-input dso-input-text dso-required">
            <div class="dso-label-container">
              <label for="single-page-form-invoer-veld-2" class="control-label">
                Webadres van uw bedrijf
              </label>
            </div>

           

            <div class="dso-field-container">
              <input type="text" id="single-page-form-invoer-veld-2" required class="form-control">
            </div>
          </div>

          <fieldset class="form-group dso-files dso-valid dso-required">
            <legend class="sr-only"></legend>
            <div class="dso-label-container">
              <label for="input-file-valid" class="control-label" aria-hidden="true">
                    Voeg een document toe waar uit blijkt dat u in het buitenland woont.
                </label>
              <button type="button" class="btn dso-info-button dso-open" aria-expanded="true">
                <span class="sr-only">Toelichting bij vraag</span>
              </button>

              <div class="dso-info">

                <button type="button">
                  <span class="sr-only">Sluiten</span>
                </button>

                <div class="dso-rich-content">
                  <p>
                    De volgende eisen zijn er voor het toevoegen van een document
                    <ul>
                      <li>blablbalblablaban</li>
                      <li>blablbalblablaban</li>
                      <li>blablbalblablaban</li>
                      <li>blablbalblablaban</li>
                    <ul>
                </p>

                </div>
              </div>
            </div>
            <div class="dso-field-container">
              <ul class="dso-filelist">

                <li>
                  <div class="dso-filename" id="input-file-valid-file-filename-0">
                            Bestandsnaam.pdf
                        </div>

                  <button type="button" aria-describedby="input-file-valid-file-filename-0" class="btn btn-link dso-remove">
                    <span>Verwijder document</span></button>
                </li>

              </ul>
              <div class="dso-file-upload">
                <input type="file" id="input-file-valid-file-upload"/>
                <label class="btn btn-default" for="input-file-valid-file-upload">
                  <dso-icon icon="plus"></dso-icon>
                  <span>Document toevoegen</span>
                </label>
              </div>

            </div>
          </fieldset>
          
          <div class="form-group dso-input dso-input-text dso-required ">
            <div class="dso-label-container">
  
            </div>

            <div class="dso-field-container">
              <div class="g-recaptcha" data-sitekey="your_site_key"></div>
            </div>
          </div>

        </fieldset>

        <div class="dso-form-buttons">

          <button type="submit" class="btn btn-primary">
            <span>Omgevingsloket-ID aanvragen</span></button>

          <button type="button" class="btn btn-link">
            <span>Annuleren</span></button>
        </div>
        
      </form>
    </div>
  </div>
  {% render '@footer' %}
</main>